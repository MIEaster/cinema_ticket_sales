<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dao.TypeDao">
    <select id="all" resultType="com.dao.TypeDao">
        select * from type
    </select>

    <insert id="addType">
        insert into type(type_name) values (#{type.typeName})
    </insert>

</mapper>